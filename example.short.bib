	Eprint = {arXiv:astro-ph/0606680},
	Journal = {\mnras},
	Keywords = {galaxies: individual: NGC 4594: galaxies: kinematics and dynamics: galaxies: spiral: galaxies: structure: dark matter, galaxies: individual: NGC 4594, galaxies: kinematics and dynamics, galaxies: spiral, galaxies: structure, dark matter},
	Month = sep,
	Pages = {1269-1279},
	Title = {{Line-of-sight velocity dispersions and a mass-distribution model of the Sa galaxy NGC 4594}},
