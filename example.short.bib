	Date-Modified = {2009-06-02 11:10:57 -0400},
	Doi = {10.1086/165567},
	Journal = {\apj},
	Keywords = {ASTROMETRY, DARK MATTER, GALACTIC STRUCTURE, MASS DISTRIBUTION, MILKY WAY GALAXY, STELLAR MOTIONS, BAYES THEOREM, ECCENTRICITY, HALOS, ORBITAL VELOCITY, RADIAL VELOCITY, STAR DISTRIBUTION},
