	Date-Added = {2010-02-05 11:18:51 -0500},
	Date-Modified = {2010-02-05 11:18:51 -0500},
	Doi = {10.1051/0004-6361/200912316},
	Eprint = {0906.4821},
	Journal = {\aap},
	Keywords = {Galaxy: disk, Galaxy: formation, galaxies: evolution, Local Group, galaxies: individual: M 31, galaxies: abundances},
