	Journal = {\mnras},
	Keywords = {methods: data analysis, methods: statistical, cosmic microwave background},
	Month = jan,
	Pages = {925-936},
	Title = {{Fast and reliable Markov chain Monte Carlo technique for cosmological parameter estimation}},
	Volume = 356,
