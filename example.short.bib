	Keywords = {Galaxies: Evolution, Galaxies: Formation, Galaxies: Kinematics and Dynamics},
	Month = jul,
	Pages = {228-239},
	Title = {{The Peculiar Velocities of Satellites of External Disk Galaxies}},
