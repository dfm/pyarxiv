	Journal = {\apj},
	Keywords = {galaxies: individual: NGC 6503, galaxies: kinematics and dynamics, galaxies: structure, methods: numerical},
	Month = jun,
	Pages = {1152-1169},
	Title = {{Dynamical Models for NGC 6503 Using a Markov Chain Monte Carlo Technique}},
	Volume = 715,
