	Eprint = {0903.2264},
	Journal = {arXiv:0903.2264},
	Keywords = {Astrophysics - Galaxy Astrophysics},
	Month = mar,
	Title = {{Galaxy Orientation and Alignment Effects in the SDSS DR6}},
	Year = 2009,
