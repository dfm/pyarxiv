	Eprint = {0711.2429},
	Journal = {\mnras},
	Keywords = {methods: numerical , Galaxy: formation , galaxies: dwarf , galaxies: kinematics and dynamics , dark matter},
	Month = apr,
	Pages = {1365-1373},
	Title = {{Infall of substructures on to a Milky Way-like dark halo}},
