	Eprint = {arXiv:astro-ph/0611896},
	Journal = {\apj},
	Keywords = {Galaxies: Formation, Galaxies: Kinematics and Dynamics, Galaxies: Star Clusters},
	Month = apr,
	Pages = {980-992},
	Title = {{Spectroscopy of Globular Clusters out to Large Radius in the Sombrero Galaxy}},
	Volume = 658,
	Year = 2007,
