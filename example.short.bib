	Doi = {10.1046/j.1365-8711.2003.06254.x},
	Eprint = {arXiv:astro-ph/0207019},
	Journal = {\mnras},
	Keywords = {galaxies: clusters: general, galaxies: formation, cosmology: theory, dark matter, large-scale structure of Universe},
