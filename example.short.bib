	Eprint = {arXiv:astro-ph/9903298},
	Journal = {\aj},
	Keywords = {GALAXIES: CLUSTERS: GENERAL, GALAXIES: KINEMATICS AND DYNAMICS, GALAXIES: SPIRAL, GALAXIES: LOCAL GROUP},
	Month = jul,
	Pages = {337-345},
