	Journal = {\apj},
	Keywords = {Galaxy: kinematics and dynamics, Galaxy: structure, methods: statistical, solar neighborhood, stars: kinematics, techniques: radial velocities},
	Month = aug,
	Pages = {1794-1819},
	Title = {{The Velocity Distribution of Nearby Stars from Hipparcos Data. I. The Significance of the Moving Groups}},
