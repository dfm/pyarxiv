	Keywords = {Cosmology: Dark Matter, Galaxies: Fundamental Parameters, Galaxies: Halos, Galaxies: Kinematics and Dynamics, Galaxies: Spiral, Galaxies: Structure},
	Month = jan,
	Pages = {218-242},
	Title = {{Mass Modeling of Disk Galaxies: Degeneracies, Constraints, and Adiabatic Contraction}},
	Volume = 619,
	Year = 2005,
