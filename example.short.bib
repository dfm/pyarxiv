	Doi = {10.1111/j.1365-2966.2007.12748.x},
	Eprint = {0710.3740},
	Journal = {\mnras},
	Keywords = {Galaxy: formation , galaxies: kinematics and dynamics , Local Group , dark matter},
	Month = mar,
	Pages = {1459-1468},
	Title = {{Masses for the Local Group and the Milky Way}},
