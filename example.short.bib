	Eprint = {0906.0397},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics, Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = jun,
	Title = {{Mass-to-Light Ratios for M31 Globular Clusters: Age-Dating and a Surprising Metallicity Trend}},
	Year = 2009,
	Bdsk-File-1 = {}}

@article{Little:1987,
	Abstract = {A new method of statistical analysis based on Bayes' theorem is devised and used directly to yield confidence intervals for the mass of the Galaxy once the eccentricity distribution of its satellites are specified. If the satellites have an isotropic velocity distribution, then the Galactic mass is less than about 5.2 x 10 to the 11th solar masses at the 95 percent confidence level. If the Galaxy is modeled as an infinite halo with constant circular speed v(c) and the velocities are isotropic, then the analysis predicts v(c) of less than about 165 km/s at the 95 percent confidence level. If the satellite orbits are more radial, then the estimates are lower. These results suggest that the Galaxy's massive dark halo extends to less than about 50 kpc from the Galactic center. },
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1987ApJ...320..493L},
