	Eprint = {arXiv:astro-ph/9605084},
	Journal = {\apj},
	Keywords = {CELESTIAL MECHANICS, STELLAR DYNAMICS, COSMOLOGY: DARK MATTER, GALAXIES: ELLIPTICAL AND LENTICULAR, CD, GALAXIES: KINEMATICS AND DYNAMICS, GALAXIES: STRUCTURE},
