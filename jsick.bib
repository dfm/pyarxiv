	Date-Modified = {2008-04-25 21:40:48 -0500},
	Doi = {10.1086/491727},
	Eprint = {arXiv:astro-ph/0504164},
	Journal = {\apj},
	Keywords = {astr452, Galaxies: Evolution- Galaxies: Individual: Messier Number: M31, Galaxies: Structure, Galaxies: Local Group},
	Month = nov,
	Pages = {287-313},
	Title = {{On the Accretion Origin of a Vast Extended Stellar Disk around the Andromeda Galaxy}},
