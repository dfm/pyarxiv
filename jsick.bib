	Doi = {10.1051/0004-6361:20020061},
	Journal = {\aap},
	Keywords = {n2392,PNe,CSPN},
	Month = mar,
	Pages = {987-998},
	Title = {{Mass-loss rates of H-rich central stars of planetary nebulae as distance indicators?}},
