	Eprint = {arXiv:astro-ph/0002395},
	Journal = {\mnras},
	Keywords = {METHODS: ANALYTICAL, GALAXIES: CLUSTERS: GENERAL, LARGE-SCALE STRUCTURE OF UNIVERSE, methods: analytical, galaxies: clusters: general, large-scale structure of Universe},
	Month = feb,
