	Journal = {ArXiv e-prints},
	Keywords = {galaxies halo},
	Local-Url = {file://localhost/Users/jsick/Documents/Papers/de-Jong2007c.pdf},
	Month = oct,
	Title = {{GHOSTS | Bulges, Halos, and the Resolved Stellar Outskirts of Massive Disk Galaxies}},
	Volume = 710,
