@article{Taoso:2008,
	Abstract = {An extraordinarily rich zoo of non-baryonic dark matter candidates has been proposed over the last three decades. Here we present a ten-point test that a new particle has to pass in order to be considered a viable DM candidate. (I) Does it match the appropriate relic density? (II) Is it cold? (III) Is it neutral? (IV) Is it consistent with BBN? (V) Does it leave stellar evolution unchanged? (VI) Is it compatible with constraints on self-interactions? (VII) Is it consistent with direct DM searches? (VIII) Is it compatible with gamma-ray constraints? (IX) Is it compatible with other astrophysical bounds? (X) Can it be probed experimentally?},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2008JCAP...03..022T},
	Archiveprefix = {arXiv},
