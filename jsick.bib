	Keywords = {Statistics - Methodology, Astrophysics - Galaxy Astrophysics, Physics - Data Analysis, Statistics and Probability, Statistics - Computation},
	Month = may,
	Primaryclass = {stat.ME},
	Title = {{Extreme deconvolution: inferring complete distribution functions from noisy, heterogeneous and incomplete observations}},
