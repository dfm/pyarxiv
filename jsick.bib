	Keywords = {wavelets},
	Month = {January},
	Number = {1},
	Pages = {61-78},
	Title = {{A Practical Guide to Wavelet Analysis}},
	Url = {http://atoc.colorado.edu/research/wavelets/},
	Volume = {79},
	Year = {1998},
	Bdsk-File-1 = {}}

