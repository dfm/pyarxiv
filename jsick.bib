	Date-Added = {2009-11-18 16:12:53 -0500},
	Date-Modified = {2009-11-18 16:12:54 -0500},
	Doi = {10.1086/376774},
	Eprint = {arXiv:astro-ph/0305257},
	Journal = {\apj},
	Keywords = {ISM: Dust, Extinction, Galaxies: ISM, Galaxies: Magellanic Clouds, Ultraviolet: ISM},
	Month = sep,
	Pages = {279-293},
	Title = {{A Quantitative Comparison of the Small Magellanic Cloud, Large Magellanic Cloud, and Milky Way Ultraviolet to Near-Infrared Extinction Curves}},
