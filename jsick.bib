	Journal = {\apjl},
	Keywords = {Galaxies: Individual: Messier Number: M31, Galaxies: Interactions, Galaxies: Kinematics and Dynamics},
	Month = jul,
	Pages = {L33-L36},
	Title = {{Was the Andromeda Stream Produced by a Disk Galaxy?}},
