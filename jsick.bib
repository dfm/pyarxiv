	Archiveprefix = {arXiv},
	Author = {{Girardi}, L. and {Williams}, B.~F. and {Gilbert}, K.~M. and {Rosenfield}, P. and {Dalcanton}, J.~J. and {Marigo}, P. and {Boyer}, M.~L. and {Dolphin}, A. and {Weisz}, D.~R. and {Melbourne}, J. and {Olsen}, K.~A.~G. and {Seth}, A.~C. and {Skillman}, E.},
