	Eprint = {arXiv:astro-ph/9911041},
	Journal = {\nat},
	Month = nov,
	Pages = {53-55},
	Title = {{Debris streams in the solar neighbourhood as relicts from the formation of the Milky Way}},
	Volume = 402,
	Year = 1999,
	Bdsk-File-1 = {}}

@article{Lynden-Bell:1995,
	Abstract = {A new method of analysis yields streams of globular clusters likely to trace the orbits of satellites that may have long since merged with the Galaxy. Besides the streams associated with the Magellanic Clouds and Fornax, new ones associated with the recently discovered remnant in Sagittarius and other possible streams of the outer-halo globular clusters are suggested. Only a few of these suggested streams will be true, but they can be tested because we are able to determine the orbits and predict the proper motions of each cluster. These predicted proper motions which are greater than 1 mas yr^-1 for 22 objects are given in Table 3 and Appendix B.},
