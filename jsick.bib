	Date-Modified = {2008-11-05 16:54:36 -0500},
	Doi = {10.1051/0004-6361:20052901},
	Journal = {\aap},
	Keywords = {stars: activity, starspots, stars: imaging, stars: individual: MN Lup, stars: individual: T Tauri, stars: pre-main sequence},
	Month = sep,
	Pages = {1105-1118},
	Rating = {5},
	Title = {{Spatially resolving the accretion shocks on the rapidly-rotating M0 T-Tauri star MN Lupi}},
