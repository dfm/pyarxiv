	Eprint = {0905.4525},
	Journal = {\apjl},
	Keywords = {galaxies: elliptical and lenticular, cD, galaxies: evolution, galaxies: fundamental parameters, galaxies: general, galaxies: interactions},
	Month = jul,
	Pages = {L9-L12},
	Title = {{The Effect of Dry Mergers on the Color-Magnitude Relation of Early-Type Galaxies}},
	Volume = 699,
