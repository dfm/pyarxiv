	Doi = {10.1086/599023},
	Eprint = {0903.2140},
	Journal = {\pasp},
	Keywords = {Data Analysis and Techniques},
	Month = apr,
	Pages = {414-427},
	Primaryclass = {astro-ph.IM},
	Title = {{Lossless Astronomical Image Compression and the Effects of Noise}},
	Volume = 121,
