	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics, Astrophysics - Galaxy Astrophysics},
	Month = apr,
	Title = {{Spectral Energy Distributions and Age Estimates of 39 Globular Clusters in M31}},
	Year = 2009,
