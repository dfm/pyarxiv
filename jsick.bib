@article{van-de-Weygaert:1994,
	Abstract = {In this paper we present a study of the statistical properties of three-dimensional Voronoi tessellations. This unique tiling of space into polyhedral cells generated by a random distribution of seeds is a phenomological model for a cell-like or sponge-like distribution of galaxies. By means of a Monte Carlo study we subsequently turn towards an extensive statistical analysis of several geometrical aspects of three dimensional Voronoi tessellations. To assess the dependence of its geometrical properties on the underlying distribution of nuclei three different distributions are investigated. We generate for each of these distributions four different realizations of tessellations, each of 1000 cells, inside a box with periodic boundary conditions. The first part of this study concerns the moments and distributions of geometrical properties of the tessellations and their building blocks, the cells. In the second part we turn to the properties of planar and linear sections through these three-dimensional tessellations.},
	Adsnote = {Provided by the Smithsonian/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/nph-bib_query?bibcode=1994A%26A...283..361V&db_key=AST},
	Author = {{van de Weygaert}, R.},
