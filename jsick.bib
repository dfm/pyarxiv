	Doi = {10.1111/j.1365-2966.2008.14239.x},
	Eprint = {0810.5530},
	Journal = {\mnras},
	Keywords = {galaxies: clusters: general , galaxies: evolution , galaxies: formation , galaxies: luminosity function, mass function , galaxies: statistics , galaxies: structure},
	Month = feb,
	Pages = {628-640},
	Title = {{Bulge-disc decompositions and structural bimodality of Ursa Major cluster spiral galaxies}},
	Volume = 393,
	Year = 2009,
	Bdsk-File-1 = {},
