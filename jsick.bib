	Adsurl = {http://adsabs.harvard.edu/abs/2006MNRAS.371.1983M},
	Author = {{Martin}, N.~F. and {Ibata}, R.~A. and {Irwin}, M.~J. and {Chapman}, S. and {Lewis}, G.~F. and {Ferguson}, A.~M.~N. and {Tanvir}, N. and {McConnachie}, A.~W.},
