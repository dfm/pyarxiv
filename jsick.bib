	Eprint = {arXiv:astro-ph/0410207},
	Journal = {\mnras},
	Keywords = {stars: AGB and post-AGB, stars: evolution, galaxies: evolution, galaxies: stellar content, cosmology: early Universe},
	Month = sep,
	Pages = {799-825},
	Title = {{Evolutionary population synthesis: models, analysis of the ingredients and application to high-z galaxies}},
