	Editor = {{A.~Wilson}},
	Keywords = {Galaxies: Individual (M101), Galaxies: Spiral, Ultraviolet: Galaxies, Infrared: Galaxies, ISM: Dust, Extinction, Scattering},
	Month = jan,
	Pages = {313-314},
	Series = {ESA Special Publication},
	Title = {{A comparative study of the spatial distribution of ultraviolet and far-infrared fluxes from M101.}},
