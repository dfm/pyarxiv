	Eprint = {arXiv:astro-ph/0109035},
	Journal = {\pasp},
	Keywords = {ISM: Dust, Extinction, Galaxies: ISM, Galaxies: Starburst, infrared: galaxies, ultraviolet: galaxies},
	Month = dec,
	Pages = {1449-1485},
	Rating = {5},
	Title = {{The Dust Opacity of Star-forming Galaxies}},
	Volume = 113,
	Year = 2001,
