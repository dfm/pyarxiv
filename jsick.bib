	Month = {October},
	Pages = {1574-1592},
	Title = {{A Study of Edge-On Galaxies with the Hubble Space Telescope Advanced Camera for Surveys. II. Vertical Distribution of the Resolved Stellar Population}},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-bib_query?bibcode=2005AJ....130.1574S&db_key=AST},
