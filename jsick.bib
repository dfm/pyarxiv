	Author = {{Rich}, R.~M. and {Reitzel}, D.~B. and {Guhathakurta}, P. and {Gebhardt}, K. and {Ho}, L.~C.},
	Date-Added = {2009-03-26 13:54:08 -0400},
	Date-Modified = {2009-03-26 13:54:09 -0400},
	Doi = {10.1086/382108},
	Eprint = {arXiv:astro-ph/0309296},
	Journal = {\aj},
	Keywords = {Galaxies: Halos, Galaxies: Stellar Content},
	Month = apr,
	Pages = {2139-2144},
	Title = {{Deep Photometry in a Remote M31 Major-Axis Field Near G1}},
	Volume = 127,
	Year = 2004,
