	Doi = {10.1086/165866},
	Journal = {\apj},
	Keywords = {ASTRONOMICAL PHOTOGRAPHY, ENERGY DISTRIBUTION, STELLAR MASS ACCRETION, STELLAR SPECTRA, T TAURI STARS, ABSORPTION SPECTRA, EMISSION SPECTRA, INFRARED SPECTRA, STELLAR FLARES, STELLAR MODELS, STELLAR RADIATION},
	Month = dec,
	Pages = {714-733},
	Title = {{Spectral energy distributions of T Tauri stars - Disk flaring and limits on accretion}},
