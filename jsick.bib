	Pages = {361-406},
	Title = {{Fragmenting the Universe. 3: The constructions and statistics of 3-D Voronoi tessellations}},
	Volume = 283,
	Year = 1994,
	Bdsk-File-1 = {}}

@article{van-de-Weygaert:1989,
	Abstract = {The large-scale structure of the universe (around 50 Mpc) can be described by means of 'Voronoi foam', which is a partitioning of space obtained by a process called Voronoi tesselation. The first results of a comparison between the statistics of Voronoi foams and the observed galaxy distribution are presented. In particular, it is shown that the spatial two-point correlation function of Voronoi vertices has a power law behavior with almost the same amplitude and slope as that of the Abell clusters. This confirms earlier expectations that Voronoi nodes are to be identified with high-density galaxy clusters in the universe.},
	Adsnote = {Provided by the Smithsonian/NASA Astrophysics Data System},
