	Month = sep,
	Pages = {149-160},
	Series = {Presented at the Society of Photo-Optical Instrumentation Engineers (SPIE) Conference},
	Title = {{Interest of thermal and mechanical modeling for cooled astronomical instruments: the example of WIRCam}},
