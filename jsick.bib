	Doi = {10.1051/0004-6361:20053012},
	Eprint = {arXiv:astro-ph/0508075},
	Journal = {\aap},
	Keywords = {radiative transfer, methods: statistical, ISM: dust, extinction, galaxies: ISM, galaxies: spiral, submillimeter},
	Month = dec,
	Pages = {101-107},
	Title = {{The opacity of spiral galaxy disks. V. Dust opacity, HI distributions and sub-mm emission}},
