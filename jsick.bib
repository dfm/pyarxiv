	Archiveprefix = {arXiv},
	Author = {{Montalto}, M. and {Seitz}, S. and {Riffeser}, A. and {Hopp}, U. and {Lee}, C.~-. and {Sch{\"o}nrich}, R.},
	Date-Added = {2009-09-17 14:19:19 -0400},
	Date-Modified = {2009-09-17 14:19:20 -0400},
	Eprint = {0907.0669},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics},
	Month = jul,
	Title = {{Dust properties in M31.I.Basic properties and a discussion on age-dependent dust heating}},
	Year = 2009,
