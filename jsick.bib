	Doi = {10.1051/0004-6361:20066772},
	Eprint = {arXiv:astro-ph/0703139},
	Journal = {\aap},
	Keywords = {stellar evolution},
	Month = jul,
	Pages = {239-263},
	Title = {{Evolution of asymptotic giant branch stars. I. Updated synthetic TP-AGB models and their basic calibration}},
