	Doi = {10.1086/522193},
	Eprint = {0708.0422},
	Journal = {\apj},
	Keywords = {Cosmology: Dark Matter, Galaxies: Formation, Galaxies: Kinematics and Dynamics, Galaxies: Spiral, Galaxies: Structure},
