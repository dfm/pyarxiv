@phdthesis{Holwerda:2005,
	Abstract = {The extinction through a spiral galaxy's disk can be measured from the number of distant galaxies identified through this disk. To calibrate this number for crowding and confusion effects, Gonzalez et al. (1998) developed the ``Synthetic Field Method''. The method has been automated in order to apply it to a sample of 32 archival HST/WFPC2 images of 29 spirals. 
Radial opacity profiles from the combined counts are presented, averaged over the entire sample, per Hubble type and for arm and disk regions. The effects of inclination, dust clouds, Hubble type and the effects of spiral arms on the disk opacity are discussed. 
Extinction in a spiral galaxy extends at least to its optical radius and is constant for a large part of the disk. The relation between apparent opacity and inclination as well as the relation between opacity and the average color of the distant galaxies are both evidence of a clumpy distribution of the dust. 
The relation between dust and stellar light is also explored. The brighter parts of a spiral disk, the arms and the center, are more opaque than the rest of the disk. A relation between surface brightness and opacity can be seen, especially in the spiral arms. 
The radial profiles of dust opacity and surface density of atomic hydrogen gas are compared. On small scales there seems to be no relation between dust and gas. Averaged over many disks, there appears to be more dust per gas column than in previous measurements.
The opacity profiles and the profiles of emission characteristic for dust are compared. This comparison indicates that a large fraction of the dust responsible for the opacity of the spiral disk is very cold.
This method and its limitations are now very well characterized. The accuracy is shown to be directly related to the surface brightness and granularity of the field. The future application of counts of distant galaxies as a probe of extinction in a foreground disk are briefly presented.},
	Author = {Holwerda, Benne W.},
	Date-Added = {2007-09-01 11:59:48 -0500},
	Date-Modified = {2007-09-01 11:59:48 -0500},
	Keywords = {dust},
	Local-Url = {file://localhost/Users/jsick/Documents/Papers/holwerdaThesis.pdf},
	School = {University of Groningen},
	Title = {{The Opacity of Spiral Galaxy Disks}},
	Url = {http://dissertations.ub.rug.nl/faculties/science/2005/b.w.holwerda/?FullItemRecord=ON},
	Year = {2005},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dissertations.ub.rug.nl/faculties/science/2005/b.w.holwerda/?FullItemRecord=ON}}

@article{Brook:2004,
	Abstract = {The disk galaxy simulated using our chemodynamic galaxy formation code, GCD+, is shown to have a thick-disk component. This is evidenced by the velocity dispersion versus age relation for solar neighborhood stars, which clearly shows an abrupt increase in velocity dispersion at a look-back time of approximately 8 Gyr, and is in excellent agreement with observation. These thick-disk stars are formed from gas that is accreted to the galaxy during a chaotic period of hierarchical clustering at high redshift. This formation scenario is shown to be consistent with observations of both the Milky Way and extragalactic thick disks.},
