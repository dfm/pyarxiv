	Archiveprefix = {arXiv},
	Author = {{Pietrinferni}, A. and {Cassisi}, S. and {Salaris}, M. and {Percival}, S. and {Ferguson}, J.~W.},
	Date-Added = {2009-03-05 00:40:33 -0500},
