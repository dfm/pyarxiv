	Adsurl = {http://adsabs.harvard.edu/abs/2009A%26A...494..933P},
	Archiveprefix = {arXiv},
	Author = {{Perina}, S. and {Barmby}, P. and {Beasley}, M.~A. and {Bellazzini}, M. and {Brodie}, J.~P. and {Burstein}, D. and {Cohen}, J.~G. and {Federici}, L. and {Fusi Pecci}, F. and {Galleti}, S. and {Hodge}, P.~W. and {Huchra}, J.~P. and {Kissler-Patig}, M. and {Puzia}, T.~H. and {Strader}, J.},
