	Keywords = {galaxies, ultraviolet sources (astronomical), infrared sources (astronomical), astronomical spectra, interstellar matter},
	Month = apr,
	Pages = {155-177},
	Series = {American Institute of Physics Conference Series},
	Title = {{Modelling the ultraviolet/submillimeter spectral energy distributions of normal galaxies}},
	Volume = 761,
	Year = 2005,
