	Adsurl = {http://adsabs.harvard.edu/abs/2001AJ....122..257K},
	Author = {{Kalirai}, J.~S. and {Richer}, H.~B. and {Fahlman}, G.~G. and {Cuillandre}, {J.-C.} and {Ventura}, P. and {D'Antona}, F. and {Bertin}, E. and {Marconi}, G. and {Durrell}, P.~R.},
