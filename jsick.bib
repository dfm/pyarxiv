	Archiveprefix = {arXiv},
	Author = {{Sand}, D.~J. and {Olszewski}, E.~W. and {Willman}, B. and {Zaritsky}, D. and {Seth}, A. and {Harris}, J. and {Piatek}, S. and {Saha}, A.},
