	Date-Added = {2008-10-07 00:00:32 -0400},
	Date-Modified = {2008-10-07 00:00:42 -0400},
	Doi = {10.1111/j.1365-2966.2008.13643.x},
	Eprint = {0803.2714},
	Journal = {\mnras},
	Keywords = {Galaxy: disc , Galaxy: formation , Galaxy: kinematics and dynamics , Galaxy: structure , galaxies: formation , galaxies: kinematics and dynamics},
	Month = sep,
	Pages = {1041-1057},
	Title = {{Thin, thick and dark discs in {$\Lambda$}CDM}},
