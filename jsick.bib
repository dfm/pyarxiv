	Eprint = {0802.1349},
	Journal = {\apj},
	Keywords = {ISM: Clouds, ISM: Structure, Radio Lines: ISM},
	Month = jun,
	Pages = {276-286},
	Title = {{The Many Streams of the Magellanic Stream}},
	Volume = 680,
