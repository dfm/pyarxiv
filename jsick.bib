	Doi = {10.1088/0004-6256/137/1/450},
	Eprint = {0807.3345},
	Journal = {\aj},
	Keywords = {dark matter, galaxies: dwarf, Local Group},
	Month = jan,
	Pages = {450-469},
	Title = {{The Invisibles: A Detection Algorithm to Trace the Faintest Milky Way Satellites}},
