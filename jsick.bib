	Eprint = {0912.3518},
	Journal = {\apjl},
	Keywords = {cosmology: observations, cosmology: theory, dark matter, galaxies: kinematics and dynamics},
	Month = feb,
	Pages = {L161-L166},
	Title = {{The Case Against Warm or Self-Interacting Dark Matter as Explanations for Cores in Low Surface Brightness Galaxies}},
