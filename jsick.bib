	Editor = {{Hibbard}, J.~E. and {Rupen}, M. and {van Gorkom}, J.~H.},
	Pages = {515-+},
	Series = {Astronomical Society of the Pacific Conference Series},
	Title = {{Distance Constraints for High-Velocity Clouds from Optical Emission Lines}},
	Volume = 240,
	Year = 2001,
