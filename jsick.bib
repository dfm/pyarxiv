	Journal = {\aj},
	Keywords = {STARS: LATE-TYPE, STARS: EVOLUTION, STARS: ABUNDANCES},
	Month = aug,
	Pages = {491-+},
	Title = {{Late-Type Stars in M31. II. C-, S-, and M-Star Spectra}},
	Volume = 112,
	Year = 1996,
