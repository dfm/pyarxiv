	Date-Modified = {2009-05-27 15:59:32 -0400},
	Doi = {10.1086/133378},
	Journal = {\pasp},
	Keywords = {ASTRONOMICAL PHOTOMETRY, COLOR-COLOR DIAGRAM, COLOR-MAGNITUDE DIAGRAM, GLOBULAR CLUSTERS, CALIBRATING, DATA REDUCTION, HUBBLE SPACE TELESCOPE},
	Month = mar,
	Pages = {250-280},
	Title = {{The center of the core-cusp globular cluster M15: CFHT and HST Observations, ALLFRAME reductions}},
	Volume = 106,
	Year = 1994,
