	Journal = {ArXiv e-prints},
	Keywords = {galaxies halo},
	Local-Url = {file://localhost/Users/jsick/Documents/Papers/Parker2007.pdf},
	Month = jul,
	Title = {{The Masses and Shapes of Dark Matter Halos from Galaxy-Galaxy Lensing in the CFHTLS}},
	Volume = 707,
	Year = 2007,
