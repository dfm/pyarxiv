@article{Dolgov:2002,
	Abstract = {We show that massive sterile neutrinos mixed with the ordinary ones may be produced in the early universe in the right amount to be natural warm dark matter particles. Their mass should be below 40 keV and the corresponding mixing angles sin<SUP>2</SUP>2theta&gt;10<SUP>-11</SUP> for mixing with nu<SUB>mu</SUB> or nu<SUB>tau</SUB>, while mixing with nu<SUB>e</SUB> is slightly stronger bounded with mass less than 30 keV.},
