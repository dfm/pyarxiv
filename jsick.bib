	Eprint = {0806.1222},
	Journal = {\mnras},
	Keywords = {methods: statistical , techniques: photometric , galaxies: evolution , galaxies: stellar content},
	Month = sep,
	Pages = {1293-1305},
	Title = {{Star formation histories from multiband photometry: a new approach}},
	Volume = 389,
	Year = 2008,
