	Doi = {10.1088/0004-6256/137/2/3000},
	Journal = {\aj},
	Keywords = {dust, extinction, galaxies: ISM, galaxies: spiral, galaxies: structure},
	Month = feb,
	Pages = {3000-3008},
	Title = {{An Extended Dust Disk in a Spiral Galaxy: An Occulting Galaxy Pair in the ACS Nearby Galaxy Survey Treasury}},
