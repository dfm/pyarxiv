	Eprint = {0711.2505},
	Journal = {\apj},
	Keywords = {Stars: Planetary Systems: Formation, Stars: Activity, stars: individual (LkCa 19), Stars: Pre-Main-Sequence},
	Month = may,
	Pages = {472-482},
	Title = {{Starspot-Induced Radial Velocity Variability in LkCa 19}},
