	Eprint = {0907.1688},
	Journal = {\mnras},
	Keywords = {galaxies: haloes , galaxies: individual: M31 , quasars: absorption lines},
	Month = oct,
	Pages = {728-736},
	Title = {{A survey of ultraviolet-bright sources behind the halo of M31}},
	Volume = 399,
	Year = 2009,
