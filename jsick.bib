	Doi = {10.1103/PhysRevD.77.065014},
	Eprint = {0711.4646},
	Journal = {\prd},
	Keywords = {Non-standard-model neutrinos, right-handed neutrinos, etc., Dark matter},
	Month = mar,
	Number = 6,
	Pages = {065014-+},
	Title = {{Dark-matter sterile neutrinos in models with a gauge singlet in the Higgs sector}},
	Volume = 77,
	Year = 2008,
	Bdsk-File-1 = {}}

@article{Feng:2010,
	Abstract = {The identity of dark matter is a question of central importance in both astrophysics and particle physics. In the past, the leading particle candidates were cold and collisionless, and typically predicted missing energy signals at particle colliders. However, recent progress has greatly expanded the list of well-motivated candidates and the possible signatures of dark matter. This review begins with a brief summary of the standard model of particle physics and its outstanding problems. We then discuss several dark matter candidates motivated by these problems, including WIMPs, superWIMPs, light gravitinos, hidden dark matter, sterile neutrinos, and axions. For each of these, we critically examine the particle physics motivations and present their expected production mechanisms, basic properties, and implications for direct and indirect detection, particle colliders, and astrophysical observations. Upcoming experiments will discover or exclude many of these candidates, and progress may open up an era of unprecedented synergy between studies of the largest and smallest observable length scales.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2010arXiv1003.0904F},
	Archiveprefix = {arXiv},
	Author = {{Feng}, J.~L.},
	Date-Added = {2010-03-05 17:59:18 -0600},
	Date-Modified = {2010-03-05 17:59:18 -0600},
	Eprint = {1003.0904},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics, High Energy Physics - Experiment, High Energy Physics - Phenomenology},
	Month = mar,
	Title = {{Dark Matter Candidates from Particle Physics and Methods of Detection}},
	Year = 2010,
	Bdsk-File-1 = {}}

@article{Cline:1992,
	Abstract = {A new formalism for damped oscillations is proposed in which the incoherent interactions are incorporated into the imaginary part of the Hamiltonian. This gives a more rigorous description of the approach of the sterile neutrino species toward thermal equilibrium than the heuristic procedure of multiplying the oscillation probability times the ordinary interaction rate to obtain the interaction rate of the sterile process. The approach proposed here is also simpler to use than the density-matrix approach.},
