	Journal = {\pasp},
	Keywords = {INSTRUMENTATION: SPECTROGRAPHS, TECHNIQUES: SPECTROSCOPIC},
	Month = oct,
	Pages = {1216-1234},
	Title = {{Sampling and Background Subtraction in Fiber-Lenslet Integral Field Spectrographs}},
	Volume = 110,
	Year = 1998,
	Bdsk-File-1 = {},
