	Journal = {\aj},
	Keywords = {galaxies halo, NGC 4631},
	Local-Url = {file://localhost/Users/jsick/Documents/Papers/2003AJ....125.1204T.pdf},
	Month = mar,
	Pages = {1204-1209},
	Title = {{The Origin of the Dust Arch in the Halo of NGC 4631: An Expanding Superbubble?}},
	Url = {http://adsabs.harvard.edu/abs/2003AJ....125.1204T},
	Volume = 125,
