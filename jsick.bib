	Keywords = {Instrumentation: Spectrographs, methods: laboratory, Techniques: Spectroscopic},
	Month = nov,
	Pages = {1564-1573},
	Title = {{Design of an Integral Field Unit for MUSE, and Results from Prototyping}},
	Volume = 118,
