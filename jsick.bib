	Archiveprefix = {arXiv},
	Author = {{Walter}, F. and {Brinks}, E. and {de Blok}, W.~J.~G. and {Bigiel}, F. and {Kennicutt}, R.~C. and {Jr.} and {Thornley}, M.~D. and {Leroy}, A.~K.},
