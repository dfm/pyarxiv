	Doi = {10.1086/427716},
	Eprint = {arXiv:astro-ph/0411663},
	Journal = {\aj},
	Keywords = {Astronomical Data Bases: Miscellaneous, ISM: Dust, Extinction, Galaxies: ISM, Galaxies: Photometry, Galaxies: Spiral, Methods: Statistical, Radiative Transfer, Techniques: Photometric},
	Month = mar,
	Pages = {1396-1411},
	Title = {{The Opacity of Spiral Galaxy Disks. IV. Radial Extinction Profiles from Counts of Distant Galaxies Seen through Foreground Disks}},
	Volume = 129,
