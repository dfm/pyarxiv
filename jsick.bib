	Eprint = {1006.4637},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = jun,
	Title = {{Modelling the spectral energy distribution of galaxies: introducing the artificial neural network}},
	Year = 2010,
