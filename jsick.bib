	Doi = {10.1086/497455},
	Eprint = {arXiv:astro-ph/0512557},
	Journal = {\pasp},
	Keywords = {Instrumentation: Spectrographs, Techniques: Photometric, Techniques: Spectroscopic},
	Month = jan,
	Pages = {129-145},
	Title = {{PMAS: The Potsdam Multi-Aperture Spectrophotometer. II. The Wide Integral Field Unit PPak}},
	Volume = 118,
	Year = 2006,
