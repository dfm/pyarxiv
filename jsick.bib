@unpublished{Sick:2009,
	Abstract = {In 2007B, we obtained 20 hours of WIRCam, 0.65\arcsec\ IQ,
observing time to image the disk of M31 at J and Ks bands out to the optical
radius ($\sim$23 Ks-mag arcsec$^{-2}$). Although the image quality is superb,
the adopted sky-target nodding sequences, based on conservative expectations,
yielded uncertain sky levels. This proposal is an attempt to salvage
exquisite data with a rapidly monitored sky background, and to extend
CFHT's WIRCam Legacy. The matching of resolved photometry from star counts
(\textit{i.e.}, colour-magnitude diagrams [CMDs]) with the first accurate NIR 
light profile of M31 will establish the first direct correspondence between 
stellar populations and integrated galaxy colours.  The age-metallicity-dust degeneracy that plagues optical-only maps is lifted with NIR images.  CMDs that probe well
into the galaxy disk will enable the first calibration of 
the contentious contribution of high-metallicity AGB stars to the K-band 
light\ --\ a crucial element for stellar population models.  The implications
of this calibration to stellar mass estimates of high-redshift galaxies
cannot be overstated.  Our J and Ks band light profiles and CMDs, when
combined with optical (CFHT MegaCam/PAndAS), infrared (Spitzer IRAC/MIPS), 
and UV (GALEX) imaging will yield an unparalleled characterization of the
dust content, age/metallicity gradients, and stellar mass in M31.},
	Author = {{Sick}, J.~N.},
