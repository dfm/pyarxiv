	Keywords = {COSMOLOGY: DIFFUSE RADIATION, ISM: DUST, EXTINCTION, GALAXIES: INDIVIDUAL MESSIER NUMBER: M31, GALAXIES: ISM, GALAXIES: PHOTOMETRY, INFRARED: GALAXIES},
	Month = jan,
	Pages = {163-+},
	Title = {{High-Resolution IRAS Maps and Infrared Emission of M31. II. Diffuse Component and Interstellar Dust}},
