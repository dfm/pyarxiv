    title = "{Exploring black hole superkicks}",
  journal = {\prd},
archivePrefix = "arXiv",
   eprint = {0707.0135},
 primaryClass = "gr-qc",
 keywords = {Numerical studies of black holes and black-hole binaries, Wave generation and sources, Relativity and gravitation, Mathematical and relativistic aspects of cosmology},
     year = 2008,
    month = jun,
   volume = 77,
   number = 12,
    pages = {124047-+},
 abstract = "{Recent calculations of the recoil velocity in black-hole binary mergers
have found kick velocities of {\ap}2500km/s for equal-mass binaries with
antialigned initial spins in the orbital plane. In general the dynamics
of spinning black holes can be extremely complicated and are difficult
to analyze and understand. In contrast, the {\ldquo}superkick{\rdquo}
configuration is an example with a high degree of symmetry that also
exhibits exciting physics. We exploit the simplicity of this test case
to study more closely the role of spin in black-hole recoil and find
that the recoil is with good accuracy proportional to the difference
between the (l=2,m={\plusmn}2) modes of {$\Psi$}_{4}, the major
contribution to the recoil occurs within 30M before and after the
merger, and that this is after the time at which a standard
post-Newtonian treatment breaks down. We also discuss consequences of
the (l=2,m={\plusmn}2) asymmetry in the gravitational wave signal for the
angular dependence of the signal-to-noise ratio and the mismatch of the
gravitational wave signals corresponding to the north and south poles.
}",
      doi = {10.1103/PhysRevD.77.124047},
   adsurl = {http://adsabs.harvard.edu/abs/2008PhRvD..77l4047B},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2008MNRAS.386.2242H,
   author = {{Hu}, J.},
    title = "{The black hole mass-stellar velocity dispersion correlation: bulges versus pseudo-bulges}",
  journal = {\mnras},
