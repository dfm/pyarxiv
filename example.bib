	Doi = {10.1111/j.1365-2966.2004.08407.x},
	Eprint = {arXiv:astro-ph/0406246},
	Journal = {\mnras},
	Keywords = {methods: statistical, galaxies: formation, galaxies: fundamental parameters, galaxies: haloes, cosmological parameters, dark matter},
