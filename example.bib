@ARTICLE{2009PhRvD..80l4051C,
   author = {{Chu}, T. and {Pfeiffer}, H.~P. and {Scheel}, M.~A.},
    title = "{High accuracy simulations of black hole binaries: Spins anti-aligned with the orbital angular momentum}",
  journal = {\prd},
archivePrefix = "arXiv",
   eprint = {0909.1313},
