@BOOK{1992grle.book.....S,
   author = {{Schneider}, P. and {Ehlers}, J. and {Falco}, E.~E.},
    title = "{Gravitational Lenses}",
 keywords = {EXTRAGALACTIC COSMOLOGY, EXTRAGALAKTISCHE KOSMOLOGIE, GEOMETRICAL OPTICS, GEOMETRISCHE OPTIK, GRAVITATION, GRAVITATION, GRAVITATIONAL LENSING, GRAVITATIONSLINSEN, QUASARE, QUASARS, REDSHIFT, ROTVERSCHIEBUNG, GRAVITATIONAL LENSES},
