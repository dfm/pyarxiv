	Journal = {\mnras},
	Keywords = {stars: evolution, galaxies: evolution, galaxies: formation, galaxies: stellar content},
	Month = oct,
	Pages = {1000-1028},
	Title = {{Stellar population synthesis at the resolution of 2003}},
	Volume = 344,
