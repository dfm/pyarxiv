	Date-Added = {2009-10-13 12:28:09 -0400},
	Date-Modified = {2009-10-13 12:28:22 -0400},
	Doi = {10.1111/j.1365-2966.2008.13828.x},
	Eprint = {0706.1350},
	Journal = {\mnras},
	Keywords = {galaxies: clusters: general , galaxies: dwarf , galaxies: formation , galaxies: haloes , galaxies: structure , dark matter},
	Month = nov,
