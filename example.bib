@ARTICLE{2007PhRvD..76h4020V,
   author = {{Vaishnav}, B. and {Hinder}, I. and {Herrmann}, F. and {Shoemaker}, D.
	},
    title = "{Matched filtering of numerical relativity templates of spinning binary black holes}",
  journal = {\prd},
