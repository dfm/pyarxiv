	Eprint = {1101.2201},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = jan,
	Primaryclass = {astro-ph.CO},
	Title = {{A Deep Photometric Look at Two of Andromeda's Dwarf Spheroidals: X and XVII}},
	Year = 2011,
