@ARTICLE{2009RvMP...81..109C,
   author = {{Castro Neto}, A.~H. and {Guinea}, F. and {Peres}, N.~M.~R. and 
	{Novoselov}, K.~S. and {Geim}, A.~K.},
    title = "{The electronic properties of graphene}",
  journal = {Reviews of Modern Physics},
archivePrefix = "arXiv",
   eprint = {0709.1163},
 keywords = {Carbon, diamond, graphite, Electron states at surfaces and interfaces, Relativistic wave equations, Thin layers, films, monolayers, membranes},
     year = 2009,
    month = jan,
   volume = 81,
    pages = {109-162},
 abstract = "{This article reviews the basic theoretical aspects of graphene, a
one-atom-thick allotrope of carbon, with unusual two-dimensional
Dirac-like electronic excitations. The Dirac electrons can be controlled
by application of external electric and magnetic fields, or by altering
sample geometry and/or topology. The Dirac electrons behave in unusual
ways in tunneling, confinement, and the integer quantum Hall effect. The
electronic properties of graphene stacks are discussed and vary with
stacking order and number of layers. Edge (surface) states in graphene
depend on the edge termination (zigzag or armchair) and affect the
physical properties of nanoribbons. Different types of disorder modify
the Dirac equation leading to unusual spectroscopic and transport
properties. The effects of electron-electron and electron-phonon
interactions in single layer and multilayer graphene are also presented.
}",
      doi = {10.1103/RevModPhys.81.109},
   adsurl = {http://adsabs.harvard.edu/abs/2009RvMP...81..109C},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2009PhRvD..79b4003S,
   author = {{Scheel}, M.~A. and {Boyle}, M. and {Chu}, T. and {Kidder}, L.~E. and 
	{Matthews}, K.~D. and {Pfeiffer}, H.~P.},
    title = "{High-accuracy waveforms for binary black hole inspiral, merger, and ringdown}",
  journal = {\prd},
archivePrefix = "arXiv",
   eprint = {0810.1767},
 primaryClass = "gr-qc",
 keywords = {Numerical relativity, Numerical studies of black holes and black-hole binaries, Gravitational waves: theory, Wave generation and sources},
     year = 2009,
    month = jan,
   volume = 79,
   number = 2,
    pages = {024003-+},
 abstract = "{The first spectral numerical simulations of 16 orbits, merger, and
ringdown of an equal-mass nonspinning binary black hole system are
presented. Gravitational waveforms from these simulations have
accumulated numerical phase errors through ringdown of {\lsim}0.1radian
when measured from the beginning of the simulation, and {\lsim}0.02radian
when waveforms are time and phase shifted to agree at the peak
amplitude. The waveform seen by an observer at infinity is determined
from waveforms computed at finite radii by an extrapolation process
accurate to {\lsim}0.01radian in phase. The phase difference between this
waveform at infinity and the waveform measured at a finite radius of
r=100M is about half a radian. The ratio of final mass to initial mass
is M_{f}/M=0.95162{\plusmn}0.00002, and the final black hole spin
is S_{f}/M_{f}^{2}=0.68646{\plusmn}0.00004.
}",
