@article{Prugniel:1997,
	Abstract = {Parametrizing their light distribution with a Sersic profile, we show that the non-homology in the structure of early-type galaxies has a measurable incidence on their scaling relations. Indeed, the Sersic exponent (i.e., the shape of the photometric profile) correlates with the residuals to the Fundamental Plane. This correlation is in excellent agreement with the theoretical expectation for isotropic galaxies. Adding together the contributions of: (i) the stellar population, (ii) the rotational support, and (iii) the non-homology, fully accounts for the tilt of the Fundamental Plane. This supports the hypothesis of a constant stellar to dynamical mass ratio in the luminous region of early-type galaxies. We confirm the existence of relations between the Sersic exponent, n, and the effective radius or the absolute magnitude, and we give indications for a saturation of this index for the bright (M_B_<-20) or large (r_e_>6.3kpc) galaxies at a value compatible with the de Vaucouleurs law (n=4).},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1997A%26A...321..111P},
	Author = {{Prugniel}, P. and {Simien}, F.},
	Date-Added = {2010-06-01 13:20:22 -0400},
	Date-Modified = {2010-06-01 13:20:22 -0400},
	Journal = {\aap},
	Keywords = {GALAXIES: ELLIPTICAL {\amp} LENTICULAR, CD, GALAXIES: FUNDAMENTAL PARAMETERS, GALAXIES: GENERAL, GALAXIES: PHOTOMETRY, GALAXIES: KINEMATICS AND DYNAMICS},
	Month = may,
	Pages = {111-122},
	Title = {{The fundamental plane of early-type galaxies: non-homology of the spatial structure.}},
	Volume = 321,
	Year = 1997,
