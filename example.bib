@ARTICLE{1982MNRAS.201..503L,
   author = {{Lyne}, A.~G. and {Anderson}, B. and {Salter}, M.~J.},
    title = "{The proper motions of 26 pulsars}",
  journal = {\mnras},
 keywords = {INTERFEROMETRY, PULSARS, RADIO ASTRONOMY, STELLAR MOTIONS, ASTRONOMICAL CATALOGS, CHRONOLOGY, ERRORS, RADIO INTERFEROMETERS, SUPERNOVA REMNANTS},
     year = 1982,
    month = nov,
   volume = 201,
    pages = {503-520},
 abstract = "{Interferometric measurements having typical errors of seven
milliarcseconds per year are presented for a complete sample of 26
pulsars. Comparison with other measurements suggests that proper motion
estimates derived from timing observations contain a source of error not
accounted for in their quoted uncertainties. The information provided by
the proper motion of the pulsars concerning the pulsar's origin, ages,
magnetic field evolution, and kinetics of formation is discussed. The
pulsars appear to have rms space velocities of about 210 km/sec in a
non-Maxwellian distribution. They are moving away from the galactic
plane, indicating an origin in low z-distance Population I progenitors.
The data indicate that pulsars are born in supernova explosions of
massive stars. The data rule out most of the suggested associations
between pulsars and supernova remnants. The derived kinetic ages are
generally less than the ages determined from the spin-down rate.
}",
   adsurl = {http://adsabs.harvard.edu/abs/1982MNRAS.201..503L},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{1982ApJ...259..302C,
