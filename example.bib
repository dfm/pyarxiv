@ARTICLE{2007PhRvL..99d1102K,
   author = {{Koppitz}, M. and {Pollney}, D. and {Reisswig}, C. and {Rezzolla}, L. and 
	{Thornburg}, J. and {Diener}, P. and {Schnetter}, E.},
    title = "{Recoil Velocities from Equal-Mass Binary-Black-Hole Mergers}",
  journal = {Physical Review Letters},
   eprint = {arXiv:gr-qc/0701163},
 keywords = {Numerical relativity, Wave generation and sources},
     year = 2007,
    month = jul,
   volume = 99,
   number = 4,
    pages = {041102-+},
 abstract = "{The final evolution of a binary-black-hole system gives rise to a recoil
velocity if an asymmetry is present in the emitted gravitational
radiation. Measurements of this effect for nonspinning binaries with
unequal masses have pointed out that kick velocities {\tilde}175km/s can
be reached for a mass ratio {\sime}0.36. However, a larger recoil can be
obtained for equal-mass binaries if the asymmetry is provided by the
spins. Using two independent methods we show that the merger of such
binaries yields velocities as large as {\tilde}440km/s for black holes
having unequal spins that are antialigned and parallel to the orbital
angular momentum.
}",
      doi = {10.1103/PhysRevLett.99.041102},
   adsurl = {http://adsabs.harvard.edu/abs/2007PhRvL..99d1102K},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
