@ARTICLE{2009PhLB..678..283B,
   author = {{Barger}, V. and {Gao}, Y. and {Keung}, {W.-Y.} and {Marfatia}, D. and 
	{Shaughnessy}, G.},
    title = "{Dark matter and pulsar signals for Fermi LAT, PAMELA, ATIC, HESS and WMAP data}",
  journal = {Physics Letters B},
archivePrefix = "arXiv",
   eprint = {0904.2001},
 primaryClass = "hep-ph",
     year = 2009,
    month = jul,
   volume = 678,
    pages = {283-292},
 abstract = "{We analyze new diffuse gamma-ray data from the Fermi Gamma-ray Space
Telescope, which do not confirm an excess in the EGRET data at galactic
mid-latitudes, in combination with measurements of electron and positron
fluxes from PAMELA, Fermi and HESS within the context of three possible
sources: dark matter (DM) annihilation or decay into charged leptons,
and a continuum distribution of pulsars. We allow for variations in the
backgrounds, consider several DM halo profiles, and account for
systematic uncertainties in data where possible. We find that all three
scenarios represent the data well. The pulsar description holds for a
wide range of injection energy spectra. We compare with ATIC data and
the WMAP haze where appropriate, but do not fit these data since the
former are discrepant with Fermi data and the latter are subject to
large systematic uncertainties. We show that for cusped halo profiles,
Fermi could observe a spectacular gamma-ray signal of DM annihilation
from the galactic center while seeing no excess at mid-latitudes.
}",
      doi = {10.1016/j.physletb.2009.06.030},
   adsurl = {http://adsabs.harvard.edu/abs/2009PhLB..678..283B},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

