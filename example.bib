@ARTICLE{2009ApJ...690..802J,
   author = {{Johansson}, P.~H. and {Naab}, T. and {Burkert}, A.},
    title = "{Equal- and Unequal-Mass Mergers of Disk and Elliptical Galaxies with Black Holes}",
