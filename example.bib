@ARTICLE{2007PhRvD..76f4034B,
   author = {{Berti}, E. and {Cardoso}, V. and {Gonzalez}, J.~A. and {Sperhake}, U. and 
	{Hannam}, M. and {Husa}, S. and {Br{\"u}gmann}, B.},
    title = "{Inspiral, merger, and ringdown of unequal mass black hole binaries: A multipolar analysis}",
  journal = {\prd},
   eprint = {arXiv:gr-qc/0703053},
 keywords = {Numerical relativity, Post-Newtonian approximation; perturbation theory; related approximations, Wave generation and sources, Classical black holes},
     year = 2007,
    month = sep,
   volume = 76,
   number = 6,
    pages = {064034-+},
 abstract = "{We study the inspiral, merger, and ringdown of unequal mass black hole
binaries by analyzing a catalogue of numerical simulations for seven
different values of the mass ratio (from q=M_{2}/M_{1}=1
to q=4). We compare numerical and post-Newtonian results by projecting
the waveforms onto spin-weighted spherical harmonics, characterized by
angular indices (l,m). We find that the post-Newtonian equations predict
remarkably well the relation between the wave amplitude and the orbital
frequency for each (l,m), and that the convergence of the post-Newtonian
series to the numerical results is nonmonotonic. To leading order, the
total energy emitted in the merger phase scales like {$\eta$}^{2}
and the spin of the final black hole scales like {$\eta$}, where
{$\eta$}=q/(1+q)^{2} is the symmetric mass ratio. We study the
multipolar distribution of the radiation, finding that odd-l multipoles
are suppressed in the equal mass limit. Higher multipoles carry a larger
fraction of the total energy as q increases. We introduce and compare
three different definitions for the ringdown starting time. Applying
linear-estimation methods (the so-called Prony methods) to the ringdown
phase, we find resolution-dependent time variations in the fitted
parameters of the final black hole. By cross correlating information
from different multipoles, we show that ringdown fits can be used to
obtain precise estimates of the mass and spin of the final black hole,
which are in remarkable agreement with energy and angular momentum
balance calculations.
}",
      doi = {10.1103/PhysRevD.76.064034},
   adsurl = {http://adsabs.harvard.edu/abs/2007PhRvD..76f4034B},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2007ARA&A..45..441M,
   author = {{Miller}, J.~M.},
    title = "{Relativistic X-Ray Lines from the Inner Accretion Disks Around Black Holes}",
  journal = {\araa},
archivePrefix = "arXiv",
   eprint = {0705.0540},
     year = 2007,
    month = sep,
   volume = 45,
    pages = {441-479},
 abstract = "{Relativistic X-ray emission lines from the inner accretion disks around
black holes are reviewed. Recent observations with the Chandra X-ray
Observatory, X-ray Multi-Mirror Mission-Newton, and Suzaku are revealing
these lines to be good probes of strong gravitational effects. A number
of important observational and theoretical developments are highlighted,
including evidence of black hole spin and effects such as gravitational
light bending, the detection of relativistic lines in stellar-mass black
holes, and evidence of orbital-timescale line flux variability. In
addition, the robustness of the relativistic disk lines against
absorption, scattering, and continuum effects is discussed. Finally,
prospects for improved measures of black hole spin and understanding the
spin history of supermassive black holes in the context of black hole
galaxy coevolution are presented. The best data and most rigorous
results strongly suggest that relativistic X-ray disk lines can drive
new explorations of General Relativity and disk physics in the future.
}",
      doi = {10.1146/annurev.astro.45.051806.110555},
   adsurl = {http://adsabs.harvard.edu/abs/2007ARA%26A..45..441M},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2007EPJC...51..677B,
   author = {{Baier}, R. and {Romatschke}, P.},
    title = "{Causal viscous hydrodynamics for central heavy-ion collisions}",
  journal = {European Physical Journal C},
   eprint = {arXiv:nucl-th/0610108},
     year = 2007,
    month = aug,
   volume = 51,
    pages = {677-687},
 abstract = "{We study causal viscous hydrodynamics in the context of central
relativistic heavy-ion collisions and provide details of a
straightforward numerical algorithm to solve the hydrodynamic equations.
It is shown that correlation functions of fluctuations provide stringent
test cases for any such numerical algorithm. Passing these tests, we
study the effects of viscosity on the temperature profile in central
heavy-ion collisions. Also, we find that it is possible to counter-act
the effects of viscosity to some extent by re-adjusting the initial
conditions. However, viscous corrections are strongest for high-mass
particles, signaling the breakdown of hydrodynamic descriptions for
large {$\eta$}/s.
}",
      doi = {10.1140/epjc/s10052-007-0308-5},
   adsurl = {http://adsabs.harvard.edu/abs/2007EPJC...51..677B},
