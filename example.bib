	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics},
	Month = mar,
	Title = {{Chandra Observation of Quiescent Low-Mass X-ray Binaries in the Globular Cluster NGC 6304}},
