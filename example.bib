	Journal = {\mnras},
	Keywords = {Journal Club: methods: numerical, methods: statistical, Local Group, galaxies: stellar content},
	Month = may,
	Pages = {91-108},
	Title = {{Numerical methods of star formation history measurement and applications to seven dwarf spheroidals}},
	Volume = 332,
