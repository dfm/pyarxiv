    title = "{Spectral classification of emission-line galaxies}",
  journal = {\apjs},
 keywords = {EMISSION SPECTRA, SEYFERT GALAXIES, SPECTRAL LINE WIDTH, SPECTRUM ANALYSIS, ACTIVE GALAXIES, ASTRONOMICAL MODELS, ASTRONOMICAL SPECTROSCOPY, H II REGIONS, HOT STARS, INTERSTELLAR EXTINCTION, PHOTOIONIZATION, STAR FORMATION},
