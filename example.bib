	Archiveprefix = {arXiv},
	Author = {{Puglielli}, D. and {Widrow}, L.~M. and {Courteau}, S.},
	Date-Added = {2010-07-07 15:58:22 -0400},
	Date-Modified = {2010-07-07 15:58:22 -0400},
	Doi = {10.1088/0004-637X/715/2/1152},
