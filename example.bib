	Keywords = {Proposal: Cosmology: Miscellaneous, Galaxies: Individual: Messier Number: M31, Galaxies: Spiral, Galaxies: Structure, Cosmology: Gravitational Lensing, methods: N-body simulations},
	Month = may,
	Pages = {311-325},
	Title = {{Disk-Bulge-Halo Models for the Andromeda Galaxy}},
