@ARTICLE{2010PhRvC..82b4908V,
   author = {{Vogel}, S. and {Torrieri}, G. and {Bleicher}, M.},
    title = "{Elliptic flow fluctuations in heavy ion collisions and the perfect fluid hypothesis}",
  journal = {\prc},
