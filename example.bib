	Author = {{Ibata}, R.~A. and {Gilmore}, G. and {Irwin}, M.~J.},
	Date-Added = {2009-04-09 11:16:05 -0400},
	Date-Modified = {2009-04-09 11:16:12 -0400},
	Eprint = {arXiv:astro-ph/9506071},
	Journal = {\mnras},
	Keywords = {STARS: CARBON, GALAXY: EVOLUTION, GLOBULAR CLUSTERS: GENERAL, GALAXIES: EVOLUTION, GALAXIES: INTERACTIONS, LOCAL GROUP},
	Month = dec,
	Pages = {781-800},
	Title = {{Sagittarius: the nearest dwarf galaxy}},
	Volume = 277,
