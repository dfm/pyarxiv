	Doi = {10.1086/324137},
	Eprint = {arXiv:astro-ph/0108474},
	Journal = {\apj},
	Keywords = {Galaxies: Kinematics and Dynamics, Galaxy: Halo, Galaxy: Structure, Galaxy: Globular Clusters: General, Galaxies: Local Group},
	Month = jan,
	Pages = {736-761},
	Title = {{On the Distribution of Orbital Poles of Milky Way Satellites}},
	Volume = 564,
	Year = 2002,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1086/324137}}

