@article{Kaiser:1983,
	Abstract = {The evolution of initially adiabatic perturbations through the epoch of recombination is calculated with due regard for the polarizing property of Thompson scattering. The emerging spectra of temperature fluctuations and polarization of the microwave background and of growing density fluctuations are presented. The fluctuating component of the microwave radiation is highly (≅ 20 per cent) polarized. If the amplitude of the density perturbations at decoupling is chosen to provide pancake formation at zf ≡ 3 then the fractional temperature fluctuation is ⪅ 10-4 provided Omegah2 ⪆ 0.1. In a low-Omega universe the fluctuations occur primarily on angular scales somewhat below those on which the best observational upper limits are found.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
