	Archiveprefix = {arXiv},
	Author = {{Komatsu}, E. and {Dunkley}, J. and {Nolta}, M.~R. and {Bennett}, C.~L. and {Gold}, B. and {Hinshaw}, G. and {Jarosik}, N. and {Larson}, D. and {Limon}, M. and {Page}, L. and {Spergel}, D.~N. and {Halpern}, M. and {Hill}, R.~S. and {Kogut}, A. and {Meyer}, S.~S. and {Tucker}, G.~S. and {Weiland}, J.~L. and {Wollack}, E. and {Wright}, E.~L.},
	Date-Added = {2010-01-20 19:57:24 -0500},
