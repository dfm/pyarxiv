	Doi = {10.1086/514333},
	Eprint = {arXiv:astro-ph/0605239},
	Journal = {\apjl},
	Keywords = {Galaxies: Bulges, Galaxies: Individual: Messier Number: M31, Galaxies: Spiral, Galaxies: Structure, Infrared: Galaxies, Galaxies: Local Group},
	Month = apr,
